<?import javafx.scene.layout.Pane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Region?>
<fx:root xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml" type="Pane"
         prefHeight="30"
         prefWidth="200"
         fx:id="parentContainer">
    <HBox prefWidth="${parentContainer.width}" prefHeight="${parentContainer.height}"
          alignment="CENTER_RIGHT">
        <Label fx:id="label" text="Label1" HBox.hgrow="ALWAYS"/>
        <Region HBox.hgrow="ALWAYS"/>
        <Button fx:id="deleteButton"/>
    </HBox>

</fx:root>